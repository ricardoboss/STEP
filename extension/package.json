{
  "name": "extension",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "vscode": "^1.90.0"
  },
  "activationEvents": [
    "onLanguage:step"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "step",
        "aliases": [
          "STEP"
        ],
        "extensions": [
          ".step"
        ]
      }
    ],
    "configurationDefaults": {
      "[languageId]": {
        "editor.semanticHighlighting.enabled": true
      }
    },
    "semanticTokenTypes": [
      {
        "id": "LiteralBoolean",
        "superType": "keyword",
        "description": "LiteralBoolean token"
      },
      {
        "id": "LiteralNull",
        "superType": "keyword",
        "description": "LiteralNull token"
      },
      {
        "id": "OpeningCurlyBracket",
        "superType": "keyword",
        "description": "OpeningCurlyBracket token"
      },
      {
        "id": "ClosingCurlyBracket",
        "superType": "keyword",
        "description": "ClosingCurlyBracket token"
      },
      {
        "id": "OpeningParentheses",
        "superType": "keyword",
        "description": "OpeningParentheses token"
      },
      {
        "id": "ClosingParentheses",
        "superType": "keyword",
        "description": "ClosingParentheses token"
      },
      {
        "id": "CommaSymbol",
        "superType": "keyword",
        "description": "CommaSymbol token"
      },
      {
        "id": "UnderscoreSymbol",
        "superType": "keyword",
        "description": "UnderscoreSymbol token"
      },
      {
        "id": "OpeningSquareBracket",
        "superType": "keyword",
        "description": "OpeningSquareBracket token"
      },
      {
        "id": "ClosingSquareBracket",
        "superType": "keyword",
        "description": "ClosingSquareBracket token"
      },
      {
        "id": "ColonSymbol",
        "superType": "keyword",
        "description": "ColonSymbol token"
      },
      {
        "id": "EndOfFile",
        "superType": "keyword",
        "description": "EndOfFile token"
      },
      {
        "id": "Error",
        "superType": "keyword",
        "description": "Error token"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -w -p .",
    "vscode:prepublish": "npm run compile"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "typescript": "^5.9.3",
    "vsce": "^2.15.0",
    "vscode": "^1.1.37",
    "vscode-languageclient": "^8.1.0"
  }
}