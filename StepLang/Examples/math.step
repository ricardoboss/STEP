number a = 10

println("min(1, 2, 3, a) = ", min(1, 2, 3, a))
println("min(-3, -2, -1) = ", min(-3, -2, -1))
println("max(1, 2, 3, a) = ", max(1, 2, 3, a))
println("abs(-1) == abs(1) = ", abs(-1) == abs(1))
println("floor(1.5) = ", floor(1.5))
println("floor(-1.5) = ", floor(-1.5))
println("ceil(1.5) = ", ceil(1.5))
println("ceil(-1.5) = ", ceil(-1.5))
println("round(1.5) = ", round(1.5))
println("round(2.5) = ", round(2.5))
println("round(1.4) = ", round(1.4))
println("clamp(1, 5, a) = ", clamp(1, 5, a))
println("clamp(1, 5, -a) = ", clamp(1, 5, -a))

seed(123456789)
println("random() = ", random())

println("interpolate(0, 10, 0.5) = ", interpolate(0, 10, 0.5))

println("sqrt(e ** 4) = ", sqrt(e ** 4))
println("sqrt(-1) = ", sqrt(-1))

println("sin(1.75 * pi) = ", sin(1.75 * pi))
println("cos(pi) = ", cos(pi))
println("tan(1.5 * pi) = ", tan(1.5 * pi))

a++
println("a++ = ", a)

a--
println("a-- = ", a)

a += 10
println("a += 10 = ", a)

a -= 10
println("a -= 10 = ", a)

a *= 10
println("a *= 10 = ", a)

a /= 10
println("a /= 10 = ", a)

a **= 10
println("a **= 10 = ", a)

a %= 10
println("a %= 10 = ", a)
